    "@types/react-loadable-visibility": "^3.0.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-select": "^4.0.16",
    "bootstrap": "^5.0.1",
    "connected-react-router": "^6.9.1",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "history": "4.10.1",
    "hoist-non-react-statics": "^3.3.2",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-datepicker": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.9.0",
    "react-icons": "^4.2.0",
    "react-loadable-visibility": "^3.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "^4.3.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "webfontloader": "^1.6.28",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=src env-cmd -f .env react-scripts start",
    "start:dev": "cross-env NODE_PATH=src env-cmd -f .env.dev react-scripts start",
    "start:staging": "cross-env NODE_PATH=src env-cmd -f .env.staging react-scripts start",
    "start:prod": "cross-env NODE_PATH=src env-cmd -f .env.production react-scripts start",
    "build:dev": "cross-env NODE_PATH=src env-cmd -f .env.dev react-scripts build",
    "build:staging": "cross-env NODE_PATH=src env-cmd -f .env.staging react-scripts build",
    "build:prod": "cross-env NODE_PATH=src env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "create": "domakey",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.1",
    "@storybook/addon-essentials": "^6.3.1",
    "@storybook/addon-links": "^6.3.1",
    "@storybook/node-logger": "^6.3.1",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.3.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/webfontloader": "^1.6.32",
    "babel-loader": "8.1.0",
    "domakey": "^0.0.6",
    "eslint-config-idiomatic": "^4.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "node-sass": "4.14.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0"
  }
}
